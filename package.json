{
  "scripts": {
    "dev": "ts-node src/app.ts",
    "test": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "^#/(.+)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "jest": "^26.6.3",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typemoq": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
